import java.util.Scanner; 
class Main 
{
public static void main(String[] args)
{ 
Scanner scan=new Scanner (System.in); 
int n=scan.nextInt();
int r,s=0; 
int count=0;
int t=n;
while (n!=0)
{
r=n%10; 
s=(s*10)+r;
n=n/10; 
} 
if (t==s)
{
System.out.println("its a palindrome"); 
} 
else 
{
System.out.println("its not palindrome");
}
}
}
